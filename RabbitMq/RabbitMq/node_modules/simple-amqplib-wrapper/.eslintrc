{
    "extends": "eslint-config-airbnb-lite",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "mocha": true,
        "es6": true
    },
    "rules": {
        "space-after-keywords": "off",
        "space-return-throw-case": "off"
    }
}